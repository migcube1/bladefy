{
    "name": "migcube1/bladefy",
    "description": "Colección de componentes Blade reutilizables inspirados en Bootstrap, para interfaces limpias y rápidas en Laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "blade"
    ],
    "autoload": {
        "psr-4": {
            "Migcube1\\Bladefy\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Miguel Angel Leyva Bejarano",
            "email": "migcube@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Migcube1\\Bladefy\\BladefyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}